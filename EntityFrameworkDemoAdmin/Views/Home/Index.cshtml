@model List<Customer>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h2>Customers:</h2>
        <hr />
        @Model.Count

        @foreach (var item in Model)
        {
            <li>@item.FirstName</li>
        }
    </div>
</body>
</html>
